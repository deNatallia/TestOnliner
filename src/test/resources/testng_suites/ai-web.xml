<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite verbose="1" name="Carina Demo Tests - AI sample tests" skipfailedinvocationcounts="false" junit="false" parallel="tests" data-provider-thread-count="50" annotations="JDK">

	<parameter name="suiteOwner" value="demo" />
	<parameter name="zafira_project" value="DEMO-AI" />
	<parameter name="jenkinsJobCreation" value="true" />
	<parameter name="jenkinsJobFolder" value="DEMO" />
	<parameter name="jenkinsJobName" value="AI-Demo-Test" />
	<parameter name="jenkinsEnvironments" value="DEMO" />
	<parameter name="jenkinsEmail" value="demo@qaprosoft.com" />


	<listeners>
		<listener
			class-name="com.qaprosoft.carina.core.foundation.retry.AnnotationTransformer" />
	</listeners>

	<test name="Login AI tests">
		<classes>
			<class name="com.qaprosoft.carina.demo.AISampleTest" />
		</classes>
	</test>

</suite>